    <bpmn2:callActivity id="_EFB745E5-5189-4BA4-99BF-574B5EC06700" drools:independent="false" drools:waitForCompletion="true" name="Fetch Max Loan Amount Configuration" calledElement="InfinityAssist.RetailFetchConfiguration">
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[Fetch Max Loan Amount Configuration]]></drools:metaValue>
        </drools:metaData>
        <drools:onExit-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[org.json.JSONArray configArray = new org.json.JSONArray(kcontext.getVariable("configurationValue").toString());
for (int i = 0; i < configArray.length(); i++) {
    org.json.JSONObject obj = configArray.optJSONObject(i);
    if (obj.has("PERSONAL.LOAN")) {
		org.json.JSONObject personalLoan = obj.optJSONObject("PERSONAL.LOAN");
		String maxAmount = personalLoan.optString("maxAmount");
		kcontext.setVariable("maxEligibleAmount", maxAmount);
	}
}]]></drools:script>
        </drools:onExit-script>
